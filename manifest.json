

{
  "manifest_version": 3,
  "name": "URL Info Viewer",
  "version": "1.0",
  "description": "Displays information about visited URLs from a backend database.",
  "permissions": ["tabs", "activeTab", "scripting"],
  "host_permissions": ["<all_urls>"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
    
  }
}



