{
  "manifest_version": 3,
  "name": "Web Scan Extension",
  "version": "1.0",
  "permissions": [
  "webRequest",
  "webRequestBlocking",
  "tabs",
  "activeTab",
  "storage",
  "scripting"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "host_permissions": ["<all_urls>"],
  "action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }
}
